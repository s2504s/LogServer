{
   "background": {
      "scripts": [ "background.js" ]
   },
   "content_scripts": [ {
      "js": [ "js/libs/jquery.min.js", "js/const.js", "js/func.js", "js/main.js", "js/messages.js" ],
      "matches": [ "https://*.ogame.gameforge.com/game/index.php*" ],
      "run_at": "document_start"
   } ],
   "description": "LogServer - storage of combat reports from OGame.",
   "icons": {
      "128": "Icon-128.png",
      "16": "Icon-16.png",
      "48": "Icon-48.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "32",
   "name": "LogServer",
   "permissions": [ "webRequest", "https://*.ogame.gameforge.com/*", "https://logserver.net/*", "https://logserver.org/*" ],
   "short_name": "LogServer",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "3.6.4",
   "web_accessible_resources": [ "css/*" ]
}
